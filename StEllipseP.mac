D=priceinfo(PrMater,"Thickness",16);   //-- Толщина столешницы Amik
position=158;
Namescr="ProtoParams";
ElemName="Эллиптическая столешница";
//----------------------------------------------------------------------
NULLOUT=pushinst(1);
macro ProtoPath+"SetEnam.mac" ElemName;
sy=sysvar(60);
macro ProtoPath+"StEllipse.mac" ;
sy=sy-sysvar(60);
if (sy==0)
{
  exit;
}
#obj group all done;
macro ProtoPath+"SetEnam.mac" ElemName;
