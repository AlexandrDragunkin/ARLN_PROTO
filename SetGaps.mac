//-- Макропрограмма устанавливает зазоры для распашных дверей
global
D_DxU		//-- Зазор по ширине для утопленных
D_DzU		//-- Зазор по высоте для утопленных
D_DxN		//-- Зазор по ширине для накладных
D_DzN		//-- Зазор по высоте для накладных
D_DxD		//-- Зазор между двойными
D_DxA   //-- Зазор по ширине между смежными дверьми
;
getpar p1,p2,p3,p4,p5,p6;

D_DxU=p1;
D_DzU=p2;
D_DxN=p3;
D_DzN=p4;
D_DxD=p5;
D_DxA=p6;
exit;