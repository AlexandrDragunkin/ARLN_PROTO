//-- Программа удаляет все элементы, сыылки на которые есть в массиве
//-- Входные параметры:
//-- g_delete - глобальный массив ссылок на удаляемые элементы
//-- ndel - количество удаляемых элементов

global g_delete;
getpar ndel;
if (ndel<=0)
{
  exit;
}
i=1;
loopi:
delete g_delete[i] done;
if (i>=ndel)
{
  exit;
}
i=i+1;
goto loopi;