//-- Макропрограмма заполняет массив ссылками на выбранные объекты
//-- Входные параметры:
//-- nk (>0!!) - количество выбранных объектов
//-- Выходные параметры:
//-- ArrObj[nk] - массив ссылок на объекты
getpar nk,ArrObj;
i=0;
loopi:
i=i+1;
ArrObj[i]=getselnum(i);
if (i<nk)
{
  goto loopi;
}

exit;